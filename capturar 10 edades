using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
namespace Arreglo
{
    class Arreglo
    {
static void Main(string[] args)
        {
            int[] edades;        
            edades = new int[10];
            for (int i = 0; i< edades.Length; i++)
            {
                Console.Write("Ingrese la edad: "+(i+1)+": ");
                edades[i] = int.Parse(Console.ReadLine());
             } Console.ReadKey();
            }
        }
}
